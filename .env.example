# Example environment variables for fastcp Docker setup

# Django settings
FASTCP_APP_SECRET=your-secret-key
DEBUG=1

# Database engine: 'sqlite', 'postgres', or 'mysql'
DJANGO_DB_ENGINE=postgres

# PostgreSQL settings
DJANGO_DB_HOST=postgres
DJANGO_DB_PORT=5432
DJANGO_DB_NAME=fastcp
DJANGO_DB_USER=fastcpuser
DJANGO_DB_PASSWORD=fastcppass

